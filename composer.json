{
  "name": "kenkataiwa/doctrine-fixtures-module",
  "description": "Zend Framework Doctrine Fixtures Module",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "data-fixture",
    "module",
    "zendframework",
    "zf"
  ],
  "homepage": "http://www.doctrine-project.org/",
  "authors": [
    {
      "name": "Kenneth Kataiwa",
      "email": "mail@kenkataiwa.com"
    }
  ],
  "require": {
    "php":                               "^5.6 || ^7.0",
    "symfony/console":                   "^2.3 || ^3.0",
    "zendframework/zend-stdlib":         "^2.7.7 || ^3.0.1",
    "zendframework/zend-hydrator":       "^1.1 || ^2.2.1",
    "zendframework/zend-mvc":            "^2.7.10 || ^3.0.1",
    "zendframework/zend-servicemanager": "^2.7.6 || ^3.1",
    "doctrine/data-fixtures":            "^1",
    "doctrine/doctrine-module":          ">=1.2",
    "doctrine/doctrine-orm-module":      ">=1"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "dev-master"
  },
  "autoload": {
    "psr-0": {
      "DoctrineFixturesModule": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    },
    "zf": {
      "component": "DoctrineFixturesModule"
    }
  }
}
